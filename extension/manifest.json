{
  "manifest_version": 2,
  "name": "SeeYouLater",
  "version": "0.1",
  "description": "Saves pages to a SeeYouLater bookmarking instance",
  "icons": {
    "48": "icons/seeyoulater-48.png",
    "96": "icons/seeyoulater-96.png"
  },
  "browser_action": {
    "default_icon": "icons/seeyoulater-32.png",
    "default_title": "SeeYouLater"
  },
  "background": {
    "scripts": ["seeyoulater.js"],
    "persistent": true
  },
  "permissions": ["tabs"]
}
